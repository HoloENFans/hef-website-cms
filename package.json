{
	"name": "hef-website-cms",
	"description": "The CMS behind websites for HoloEN fan projects",
	"version": "1.0.0",
	"main": "dist/server.js",
	"license": "MIT",
	"scripts": {
		"dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
		"build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
		"build:server": "tsc",
		"build": "pnpm build:payload && pnpm build:server",
		"serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
		"generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
		"prepare": "husky install",
		"lint": "eslint .",
		"lint:fix": "eslint --fix ."
	},
	"lint-staged": {
		"*.{js,ts}": [
			"eslint --fix"
		]
	},
	"engines": {
		"node": ">=18.12.1 <19",
		"pnpm": ">=8"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "3.360.0",
		"@aws-sdk/lib-storage": "3.360.0",
		"@payloadcms/plugin-cloud-storage": "1.0.19",
		"dotenv": "16.3.1",
		"express": "4.18.2",
		"payload": "1.15.8",
		"sanitize-filename": "1.6.3"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.6",
		"@commitlint/config-conventional": "^17.6.6",
		"@types/express": "4.17.17",
		"@types/node": "20.3.2",
		"@typescript-eslint/eslint-plugin": "5.60.1",
		"@typescript-eslint/parser": "5.60.1",
		"all-contributors-cli": "^6.26.0",
		"cross-env": "7.0.3",
		"eslint": "8.43.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-airbnb-typescript": "17.0.0",
		"eslint-plugin-import": "2.27.5",
		"husky": "^8.0.0",
		"lint-staged": "^13.2.3",
		"nodemon": "2.0.22",
		"process": "0.11.10",
		"ts-node": "10.9.1",
		"typescript": "5.1.5"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/HoloENFans/hef-website-cms.git"
	},
	"author": "GoldElysium",
	"bugs": {
		"url": "https://github.com/HoloENFans/hef-website-cms/issues"
	},
	"homepage": "https://github.com/HoloENFans/hef-website-cms#readme"
}
